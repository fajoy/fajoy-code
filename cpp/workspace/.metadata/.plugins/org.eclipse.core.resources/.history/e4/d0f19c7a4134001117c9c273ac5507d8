#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>
#include <netinet/in.h>
#include <sys/socket.h>
#include <arpa/inet.h>
#include <netdb.h>
#include <iostream>
using namespace std;


class myIpRange{
public:
	unsigned int minIp;
	unsigned int maxIp;
	char c1[3],c2[3],c3[3],c4[3];
	unsigned char i1,i2,i3,i4;
	myIpRange(){
		minIp=0x00000000;
		maxIp=0xffffffff;
	}

	void parse(char * ip_str){
		sscanf(ip_str,"%[0-9\*].%[0-9\*].%[0-9\*].%[0-9\*]",c1,c2,c3,c4);
		printf("%s %s %s %s",c1,c2,c3,c4);
		i1=(unsigned char)(atoi(c1)%256);
		i2=(unsigned char)(atoi(c2)%256);
		i3=(unsigned char)(atoi(c3)%256);
		i4=(unsigned char)(atoi(c4)%256);
		printf("%d %d %d %d",i1,i2,i3,i4);
	}
};
int main() {
	char ip[]="0.0.0.32";
	//unsigned int ip_i = inet_addr(ip);
	myIpRange tmp=myIpRange();
	tmp.parse(ip);
	fflush(stdout);
	return 0;
}
